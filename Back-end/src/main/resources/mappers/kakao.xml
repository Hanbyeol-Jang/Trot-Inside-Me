<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 	
		 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.web.curation.dao.KakaoDao">
	<select id="isid" resultType="Boolean">
	select Count(*) from Kakao
	where 1=1
	and k_id = #{id}
	</select>

	<insert id="insertkakao">
		insert into Kakao (k_id,k_nickname,k_thumbnail_image,k_profile_image)
		values (#{id},#{nickname},#{thumbnail_image},#{profile_image})
	</insert>
</mapper>
        