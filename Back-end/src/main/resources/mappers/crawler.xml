<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 	
		 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.web.curation.dao.CrawlingNaverDao">
	
	<!-- 비디오 추가 -->
	<insert id="addvideo">
		insert into Video (v_url,v_title,v_content,v_member)
		values(#{v_url},#{v_title},#{v_content},#{v_member})
	</insert>
	
	<!-- 기사 추가 -->
	<insert id="addarticle">
		insert into Article (a_url,a_title,a_content,a_member)
		values(#{a_url},#{a_title},#{a_content},#{a_member})
	</insert>
	
	<!-- 비디오 리스트 조회 -->
	<select id="selectvideolist" resultType="VideoDto">
		select * 
		from Video
		where 1=1
		and v_member = #{v_member}
	</select>	
</mapper>